<div class="show-layout">
  <h3>Hello, <%= current_user.first_name %></h3>
  <div class="graph" id="bar"><%= column_chart(current_user.bars) %></div>
  <div class="graph" id="line"><%= line_chart(current_user.lines, min: 50, max: 80, :library => {
    legend: {position: 'bottom'},
    curveType: 'none',
      explorer: {keepInBounds: true, axis: 'horizontal'}
    }) %></div>
</div>